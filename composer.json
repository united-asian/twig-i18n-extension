{
    "name": "uam/twig-i18n-extension",
    "description": "Twig i18n extension",
    "keywords": ["twig", "i18n"],
    "homepage": "http://gitlab.united-asian.com/twig/i18n-extension",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Olivier Pichon",
            "email": "op@united-asian.com"
        }
    ],
    "require": {
        "twig/twig": "~1.12",
        "symfony/intl": "~2.3",
        "symfony/translation": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "4.6.*"
    },
    "autoload": {
        "psr-4": {
            "UAM\\Twig\\Extension\\I18n\\": "src/",
            "UAM\\Twig\\Extension\\I18n\\Test\\": "test/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
